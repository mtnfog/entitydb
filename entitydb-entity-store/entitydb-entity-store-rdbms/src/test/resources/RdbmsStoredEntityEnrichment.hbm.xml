<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 
<hibernate-mapping package="com.mtnfog.entitydb.entitystore.rdbms.model">
   <class name="RdbmsStoredEntityEnrichment" table="EntityEnrichments">
      <id name="id" type="int" column="id">
         <generator class="native"/>
      </id>
      <property name="name" column="name" type="string" not-null="true"/>
      <property name="value" column="value" type="string" not-null="true"/>
      <many-to-one name="entity" class="RdbmsStoredEntity" fetch="select">
       	<column name="entity" not-null="true" />
	  </many-to-one>      
   </class>
</hibernate-mapping>